{
  "name": "snyk-take-home",
  "version": "1.0.0",
  "description": "Take-home test from Snyk",
  "scripts": {
    "start": "NODE_ENV=production; nodemon --watch '**/*.ts' --exec \"ts-node\" server.ts",
    "lint": "eslint . --ext .js,.ts",
    "pretest": "npm run lint",
    "test": "nyc ts-mocha test/**/*-test.ts test/*-test.ts",
    "posttest": "jsdoc --recurse lib/ persistence/ routes/ utils/ --destination ./docs --package ./package.json --readme ./README.md"
  },
  "author": "Teodora Sandu",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.6",
    "got": "^11.8.1",
    "logging": "^3.3.0",
    "node-cache": "^5.1.2",
    "nodemon": "^2.0.7",
    "semver": "^7.3.4",
    "ts-node": "^9.1.1",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.35",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "chai": "^4.3.0",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "jsdoc": "^3.6.6",
    "mocha": "^8.3.0",
    "nock": "^13.0.7",
    "nyc": "^15.1.0",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.5.0",
    "supertest": "^6.1.3",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "nyc": {
    "include": [
      "lib/**/*.ts",
      "persistence/**/*.ts",
      "routes/**/*.ts",
      "utils/**/*.ts",
      "app.ts"
    ],
    "exclude": [
      "**/*-test.ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "all": true,
    "report-dir": "reports/coverage"
  }
}
